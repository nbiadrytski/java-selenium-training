<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Data Driven Basics" >
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
        <listener class-name="com.seleniumjava.listeners.CustomListeners"/>
    </listeners>
    <test name="Bank Manager Login">
        <classes>
            <class name="com.seleniumjava.testcases.BankManagerLoginTest"/>
        </classes>
    </test>
    <test name="Add Customer Test">
        <classes>
            <class name="com.seleniumjava.testcases.AddCustomerTest"/>
        </classes>
    </test>
    <test name="Open Account Test">
        <classes>
            <class name="com.seleniumjava.testcases.OpenAccountTest"/>
        </classes>
    </test>
</suite>
